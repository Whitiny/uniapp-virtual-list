{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"enablePullDownRefresh": false,
				"app-plus": {
					"bounce":"none"
				},
				"mp-alipay": {
					"allowsBounceVertical":"NO"
				}
			}
		}
	    ,{
            "path" : "pages/fixed-size/fixed-size",
            "style" :                                                                                    
            {
				"navigationBarTitleText": "固定高度列表",
                "enablePullDownRefresh": false,
				"app-plus": {
					"bounce":"none"
				},
				"mp-alipay": {
					"allowsBounceVertical":"NO"
				}
            }
            
        }
        ,{
            "path" : "pages/multiple-tabs/multiple-tabs",
            "style" :                                                                                    
            {
				"navigationBarTitleText": "多选项卡列表",
                "enablePullDownRefresh": false,
				"app-plus": {
					"bounce":"none"
				},
				"mp-alipay": {
					"allowsBounceVertical":"NO"
				},
				"mp-toutiao": {
					"enablePullDownRefresh": true
				}
            }
            
        }
        ,{
            "path" : "pages/dynamic-size/dynamic-size",
            "style" :                                                                                    
            {
				"navigationBarTitleText": "动态高度列表",
                "enablePullDownRefresh": false,
				"app-plus": {
					"bounce":"none"
				},
				"mp-alipay": {
					"allowsBounceVertical":"NO"
				}
            }
            
        }
        ,{
            "path" : "pages/horizontal/horizontal",
            "style" :                                                                                    
            {
				"navigationBarTitleText": "横向列表",
                "enablePullDownRefresh": false,
				"app-plus": {
					"bounce":"none"
				},
				"mp-alipay": {
					"allowsBounceVertical":"NO"
				}
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "虚拟列表",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#FFFFFF"
	}
}
